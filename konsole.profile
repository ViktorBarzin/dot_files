[Appearance]
ColorScheme=Linux
Font=Monospace,9,-1,5,50,0,0,0,0,0
UseFontLineChararacters=true

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/usr/bin/tmux new -A -s main
Environment=
Icon=utilities-terminal
LocalTabTitleFormat=%d : %n
Name=Main profile
Parent=FALLBACK/
